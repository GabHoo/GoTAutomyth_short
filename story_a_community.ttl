@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Lannister ;
    HERO:hasOccupation HERO:murderer ;
    HERO:hasTitle HERO:Lord ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Castle_Black ;
    sem:hasTime HERO:christmas .

HERO:Event_04 a HERO:MeetingTheMentor,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Meeting the Mentor" ;
    HERO:journeyStage "4" ;
    HERO:meetsMentor HERO:Cassana_Baratheon ;
    HERO:powerLearned HERO:slowingTime ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Kings_Landing ;
    sem:hasTime HERO:theMiddleOftheNight .

HERO:cause_of_death rdfs:range xsd:string .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:meetsMentor rdfs:range HERO:Mentor .

HERO:powerLearned rdfs:range HERO:HeroPower .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:Cassana_Baratheon a sem:Actor ;
    rdfs:label "Cassana Baratheon" .

HERO:Castle_Black a sem:Place ;
    rdfs:label "Castle Black" .

HERO:House_Lannister a HERO:House ;
    rdfs:label "House Lannister" .

HERO:Kings_Landing a sem:Place ;
    rdfs:label "Kings Landing" .

HERO:Lord a HERO:Title ;
    rdfs:label "Lord" .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" .

HERO:christmas a sem:Time ;
    rdfs:label "Christmas" .

HERO:murderer a HERO:Occupation ;
    rdfs:label "murderer" .

HERO:slowingTime a HERO:Power ;
    rdfs:label "slowing Time" .

HERO:theMiddleOftheNight a sem:Time ;
    rdfs:label "the Middle Of the Night" .

HERO:Tyrion_Lannister a HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Tyrion Lannister" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

