@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Targaryen ;
    HERO:hasOccupation HERO:squire ;
    HERO:hasTitle HERO:king ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Jon_Snow ;
    sem:hasPlace HERO:Beach ;
    sem:hasTime HERO:evening .

HERO:Event_04 a HERO:MeetingTheMentor,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Meeting the Mentor" ;
    HERO:journeyStage "4" ;
    HERO:meetsMentor HERO:Myrcella_Baratheon ;
    HERO:powerLearned HERO:slowingTime ;
    sem:hasActor HERO:Jon_Snow ;
    sem:hasPlace HERO:Castle_Black ;
    sem:hasTime HERO:lunch .

HERO:cause_of_death rdfs:range xsd:string .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:meetsMentor rdfs:range HERO:Mentor .

HERO:powerLearned rdfs:range HERO:HeroPower .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:Beach a sem:Place ;
    rdfs:label "Beach" .

HERO:Castle_Black a sem:Place ;
    rdfs:label "Castle Black" .

HERO:House_Targaryen a HERO:House ;
    rdfs:label "House Targaryen" .

HERO:Myrcella_Baratheon a sem:Actor ;
    rdfs:label "Myrcella Baratheon" .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" .

HERO:evening a sem:Time ;
    rdfs:label "evening" .

HERO:lunch a sem:Time ;
    rdfs:label "lunch" .

HERO:slowingTime a HERO:Power ;
    rdfs:label "slowing Time" .

HERO:squire a HERO:Occupation ;
    rdfs:label "squire" .

HERO:Jon_Snow a HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Jon Snow" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

