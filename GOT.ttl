#################################################################

###  Property Definitions ###
:father rdf:type owl:ObjectProperty ;
            rdfs:label "father"@en; 
	    owl:inverseOf :hasChild ;
	    rdfs:subPropertyOf :relative.

:mother rdf:type owl:ObjectProperty ;
            rdfs:label "mother"@en; 
	    owl:inverseOf :hasChild ;
	    rdfs:subPropertyOf :relative.

:sibling rdf:type owl:ObjectProperty , owl:SymmetricProperty;
            rdfs:label "sibling"@en; 
	    rdfs:subPropertyOf :relative.

:partner rdf:type owl:ObjectProperty , owl:SymmetricProperty;
            rdfs:label "partner"@en .

:spouse rdf:type owl:ObjectProperty , owl:SymmetricProperty;
            rdfs:label "spouse"@en .

:relative rdf:type owl:ObjectProperty , owl:SymmetricProperty ;
            rdfs:label "relative"@en.

:family rdf:type owl:ObjectProperty ;
            rdfs:label "family"@en .

:title rdf:type owl:ObjectProperty ;
	    rdfs:label "title"@en .

:occupation rdf:type owl:ObjectProperty ;
	    rdfs:label "occupation"@en .

:battles_participated rdf:type owl:ObjectProperty ;
	    rdfs:label "battles_participated"@en .

:place_of_death rdf:type owl:ObjectProperty ;
	    rdfs:label "place_of_death"@en .

:cause_of_death rdf:type owl:DatatypeProperty ;
	    rdfs:label "cause_of_death"@en ;
	    rdfs:range xsd:string.


	    
#################################################################

###  Class Definitions ###

:Main_Character rdf:type owl:Class ;
	rdfs:label "Main_Character"@en ;
	rdfs:subClassOf sem:Actor.

:Occupation rdf:type owl:Class ;
	rdfs:label "Occupation"@en .

:Battle rdf:type owl:Class ;
	rdfs:label "Battle"@en ;
	rdfs:subClassOf sem:Event .

