    rdfs:label "Daenerys Targaryen" .

HERO:Great_Sept_of_Baelor a sem:Place ;
    rdfs:label "Great Sept of Baelor" .

HERO:House_Baratheon a HERO:House ;
    rdfs:label "House Baratheon" .

HERO:InmostCave rdfs:subClassOf sem:Event .

HERO:Karhold a sem:Place ;
    rdfs:label "Karhold" .

HERO:King a HERO:Male,
        HERO:Title,
        sem:Actor ;
    rdfs:label "King" .

HERO:Kings_Landing a sem:Place ;
    rdfs:label "Kings Landing" .

HERO:MeetingTheMentor rdfs:subClassOf sem:Event .

HERO:Ordeal rdfs:subClassOf sem:Event .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" ;
    rdfs:subClassOf sem:Event .

HERO:Party rdfs:subClassOf sem:Event .

HERO:Red_Keep a sem:Place ;
    rdfs:label "Red Keep" .

HERO:RefusalOfCall rdfs:subClassOf sem:Event .

HERO:Resurrection rdfs:subClassOf sem:Event .

HERO:Reward rdfs:subClassOf sem:Event .

HERO:RoadBack rdfs:subClassOf sem:Event .

HERO:Steffon_Baratheon a HERO:Male,
        sem:Actor ;
    rdfs:label "Steffon Baratheon" .

HERO:TestAlly rdfs:subClassOf sem:Event .

HERO:The_Vale_of_Arryn a sem:Place ;
    rdfs:label "The Vale of Arryn" .

HERO:Winterfell a sem:Place ;
    rdfs:label "Winterfell" .

HERO:equality a HERO:ThreatTarget ;
    rdfs:label "equality" .

HERO:game a HERO:Celebration ;
    rdfs:label "playing a game" .

HERO:pausingTime a HERO:Power ;
    rdfs:label "pausing Time" .

HERO:slowingTime a HERO:Power ;
    rdfs:label "slowing Time" .

HERO:soldier a HERO:Occupation ;
    rdfs:label "soldier" .

HERO:strange a HERO:Feelings ;
    rdfs:label "strange" .

HERO:the_Winter_Solstice a sem:Time ;
    rdfs:label "the Winter Solstice" .

HERO:Bear_Island a sem:Place ;
    rdfs:label "Bear Island" .

HERO:Saltpans a sem:Place ;
    rdfs:label "Saltpans" .

HERO:Viserys_Targaryen a HERO:Male,
        sem:Actor ;
    rdfs:label "Viserys Targaryen" .

HERO:fireResistance a HERO:Power ;
    rdfs:label "fire Resistance" .

HERO:the_Autumnal_Equinox a sem:Time ;
    rdfs:label "the Autumnal Equinox" .

HERO:Riverlands a sem:Place ;
    rdfs:label "Riverlands" .

HERO:easter a sem:Time ;
    rdfs:label "Easter day" .

HERO:sunrise a sem:Time ;
    rdfs:label "sunrise" .

HERO:theMiddleOftheNight a sem:Time ;
    rdfs:label "the Middle Of the Night" .

HERO:Robert_Baratheon a HERO:Main_Character,
        HERO:Male,
        sem:Actor ;
    rdfs:label "Robert Baratheon" .

