    rdfs:label "House Lannister" .

HERO:InmostCave rdfs:subClassOf sem:Event .

HERO:Kings_Landing a sem:Place ;
    rdfs:label "Kings Landing" .

HERO:Lady a HERO:Female,
        HERO:Title,
        sem:Actor ;
    rdfs:label "Lady" .

HERO:MeetingTheMentor rdfs:subClassOf sem:Event .

HERO:Oldstones a sem:Place ;
    rdfs:label "Oldstones" .

HERO:Ordeal rdfs:subClassOf sem:Event .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" ;
    rdfs:subClassOf sem:Event .

HERO:Party rdfs:subClassOf sem:Event .

HERO:Raventree_Hall a sem:Place ;
    rdfs:label "Raventree Hall" .

HERO:RefusalOfCall rdfs:subClassOf sem:Event .

HERO:Resurrection rdfs:subClassOf sem:Event .

HERO:Reward rdfs:subClassOf sem:Event .

HERO:Riverlands a sem:Place ;
    rdfs:label "Riverlands" .

HERO:RoadBack rdfs:subClassOf sem:Event .

HERO:Robert_Arryn a HERO:Male,
        sem:Actor ;
    rdfs:label "Robert Arryn" .

HERO:TestAlly rdfs:subClassOf sem:Event .

HERO:The_Iron_Island a sem:Place ;
    rdfs:label "The Iron Island" .

HERO:ball a HERO:Celebration ;
    rdfs:label "throwing a ball" .

HERO:christmas a sem:Time ;
    rdfs:label "Christmas" .

HERO:enraged a HERO:Feelings ;
    rdfs:label "enraged" .

HERO:invisibility a HERO:Power ;
    rdfs:label "invisibility" .

HERO:murderer a HERO:Occupation ;
    rdfs:label "murderer" .

HERO:teleportation a HERO:Power ;
    rdfs:label "teleportation" .

HERO:the_first_day_of_Spring a sem:Time ;
    rdfs:label "the first day of Spring" .

HERO:Bran_Stark a HERO:Main_Character,
        HERO:Male,
        sem:Actor ;
    rdfs:label "Bran Stark" .

HERO:Seagard a sem:Place ;
    rdfs:label "Seagard" .

HERO:easter a sem:Time ;
