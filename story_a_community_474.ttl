HERO:The_Dreadfort a sem:Place ;
    rdfs:label "The Dreadfort" .

HERO:Torrhen_Square a sem:Place ;
    rdfs:label "Torrhen's Square" .

HERO:castle a HERO:ThreatTarget ;
    rdfs:label "castle" .

HERO:christmas a sem:Time ;
    rdfs:label "Christmas" .

HERO:fictional_king a HERO:Male,
        HERO:Title,
        sem:Actor ;
    rdfs:label "King" .

HERO:fireResistance a HERO:Power ;
