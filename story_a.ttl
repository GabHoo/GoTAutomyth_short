@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Arryn ;
    HERO:hasOccupation HERO:military_leader ;
    HERO:hasTitle HERO:queen_consort ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:Karhold ;
    sem:hasTime HERO:the_Winter_Solstice .

HERO:Event_02 a HERO:CallToAdventure,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain threatens the safety of somebody. This is the call for the hero" ;
    HERO:ThreatenedElement HERO:lifestyle ;
    HERO:journeyStage "2" ;
    HERO:villain HERO:Tyrion_Lannister ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:Deepwood_Motte ;
    sem:hasTime HERO:easter .

HERO:ThreatenedElement rdfs:range HERO:ThreatTarget .

HERO:cause_of_death rdfs:range xsd:string .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:villain rdfs:range HERO:Villain .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:Deepwood_Motte a sem:Place ;
    rdfs:label "Deepwood Motte" .

HERO:House_Arryn a HERO:House ;
    rdfs:label "House Arryn" .

HERO:Karhold a sem:Place ;
    rdfs:label "Karhold" .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" .

HERO:Tyrion_Lannister a HERO:Main_Character,
        HERO:Male,
        sem:Actor ;
    rdfs:label "Tyrion Lannister" .

HERO:easter a sem:Time ;
    rdfs:label "Easter day" .

HERO:lifestyle a HERO:ThreatTarget ;
    rdfs:label "lifestyle" .

HERO:military_leader a HERO:Occupation ;
    rdfs:label "military leader" .

HERO:queen_consort a HERO:Female,
        HERO:Title,
        sem:Actor ;
    rdfs:label "Queen " .

HERO:the_Winter_Solstice a sem:Time ;
    rdfs:label "the Winter Solstice" .

HERO:Cersei_Lannister a HERO:Female,
        HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Cersei Lannister" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

