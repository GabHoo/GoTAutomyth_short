@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Title a HERO:ThreatTarget,
        rdfs:Class ;
    rdfs:label "Title" .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Martell ;
    HERO:hasOccupation HERO:soldier ;
    HERO:hasTitle HERO:fictional_princess ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Arya_Stark ;
    sem:hasPlace HERO:Karhold ;
    sem:hasTime HERO:the_Winter_Solstice .

HERO:Event_02 a HERO:CallToAdventure,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain threatens the safety of somebody. This is the call for the hero" ;
    HERO:ThreatenedElement HERO:Title ;
    HERO:journeyStage "2" ;
    HERO:villain HERO:Robb_Stark ;
    sem:hasActor HERO:Arya_Stark ;
    sem:hasPlace HERO:The_Eyrie ;
    sem:hasTime HERO:the_Autumnal_Equinox .

HERO:ThreatenedElement rdfs:range HERO:ThreatTarget .

HERO:cause_of_death rdfs:range xsd:string .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:villain rdfs:range HERO:Villain .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:House_Martell a HERO:House ;
    rdfs:label "House Martell" .

HERO:Karhold a sem:Place ;
    rdfs:label "Karhold" .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" .

HERO:Robb_Stark a HERO:Male,
        sem:Actor ;
    rdfs:label "Robb Stark" .

HERO:The_Eyrie a sem:Place ;
    rdfs:label "The Eyrie" .

HERO:fictional_princess a HERO:Female,
        HERO:Title,
        sem:Actor ;
    rdfs:label "Princess" .

HERO:soldier a HERO:Occupation ;
    rdfs:label "soldier" .

HERO:the_Autumnal_Equinox a sem:Time ;
    rdfs:label "the Autumnal Equinox" .

HERO:the_Winter_Solstice a sem:Time ;
    rdfs:label "the Winter Solstice" .

HERO:Arya_Stark a HERO:Female,
        HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Arya Stark" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

