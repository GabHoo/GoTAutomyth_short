@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:House a HERO:ThreatTarget,
        rdfs:Class ;
    rdfs:label "House" ;
    rdfs:subClassOf HERO:House .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Tyrell ;
    HERO:hasOccupation HERO:soldier ;
    HERO:hasTitle HERO:Lord ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Great_Sept_of_Baelor ;
    sem:hasTime HERO:theMiddleOftheNight .

HERO:Event_02 a HERO:CallToAdventure,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain threatens the safety of somebody. This is the call for the hero" ;
    HERO:Threatens HERO:lifestyle ;
    HERO:journeyStage "2" ;
    HERO:villain HERO:Robert_Baratheon ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Red_Keep ;
    sem:hasTime HERO:theMiddleOftheNight .

HERO:Event_03 a HERO:RefusalOfCall,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero refuses to help saving the target and the world" ;
    HERO:journeyStage "3" ;
    HERO:refusesToFight HERO:Robert_Baratheon ;
    HERO:scaredOf HERO:fireResistance ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Saltpans ;
    sem:hasTime HERO:the_Autumnal_Equinox .

HERO:Event_04 a HERO:MeetingTheMentor,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Meeting the Mentor" ;
    HERO:journeyStage "4" ;
    HERO:meetsMentor HERO:Aerys_II_Targaryen ;
    HERO:powerLearned HERO:fireResistance ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:The_Eyrie ;
    sem:hasTime HERO:theMiddleOftheNight .

HERO:Event_05 a HERO:CrossThreshold,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The hero decides to face the villain and travels to the Place where the villain is." ;
    HERO:journeyStage "5" ;
    HERO:travelsTo HERO:Great_Sept_of_Baelor ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Torrhen_Square ;
    sem:hasTime HERO:easter .

HERO:Event_06 a HERO:TestAlly,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The ally help the hero to face the challenge against the Villain and its ally. it is a firt test" ;
    HERO:faces HERO:Cassana_Baratheon ;
    HERO:journeyStage "6" ;
    HERO:meetsAlly HERO:Euron_Greyjoy ;
    HERO:victory false ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Torrhen_Square ;
    sem:hasTime HERO:easter .

HERO:Event_07 a HERO:InmostCave,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The hero is arrived at the location to face the villain and feel strong emotions" ;
    HERO:feeling HERO:disgusted ;
    HERO:journeyStage "7" ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:The_Trident ;
    sem:hasTime HERO:easter .

HERO:Event_08 a HERO:Ordeal,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero fights the enemy with powers" ;
    HERO:enemyusespower HERO:mindReading ;
    HERO:herofights HERO:Robert_Baratheon ;
    HERO:herogetsinjured true ;
    HERO:herousespower HERO:gravityControl ;
    HERO:journeyStage "8" ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:White_Harbor ;
    sem:hasTime HERO:theMiddleOftheNight .

HERO:Event_09 a HERO:Reward,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero  SAVES WHATEVER WAS THE TARGET OF THE THREAT SO HE GETS IT BACK " ;
    HERO:journeyStage "9" ;
    HERO:saves HERO:House ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Seagard ;
    sem:hasTime HERO:sunrise .

HERO:Event_10 a HERO:RoadBack,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain chases hero down and seek for final battle" ;
    HERO:chasedBy HERO:Robert_Baratheon ;
    HERO:journeyStage "10" ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:The_Ruby_Ford ;
    sem:hasTime HERO:the_Winter_Solstice .

HERO:Event_11 a HERO:Resurrection,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Final confrontation between hero and villain" ;
    HERO:against HERO:Robert_Baratheon ;
    HERO:journeyStage "11" ;
    HERO:killsVillain false ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:Seagard ;
    sem:hasTime HERO:the_Summer_Solstice .

HERO:Event_12 a HERO:Party,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero throws a party" ;
    HERO:celebratesvictory HERO:feast ;
    HERO:journeyStage "12" ;
    HERO:partywith HERO:Euron_Greyjoy ;
    sem:hasActor HERO:Tyrion_Lannister ;
    sem:hasPlace HERO:The_Dreadfort ;
    sem:hasTime HERO:christmas .

