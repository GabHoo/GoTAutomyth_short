HERO:The_Trident a sem:Place ;
    rdfs:label "The Trident" .

HERO:The_Vale_of_Arryn a sem:Place ;
    rdfs:label "The Vale of Arryn" .

HERO:The_Whispering_Wood a sem:Place ;
    rdfs:label "The Whispering Wood" .

HERO:Viserys_Targaryen a HERO:Male,
        sem:Actor ;
    rdfs:label "Viserys Targaryen" .

HERO:White_Harbor a sem:Place ;
    rdfs:label "White Harbor" .

HERO:ball a HERO:Celebration ;
    rdfs:label "throwing a ball" .

HERO:exhausted a HERO:Feelings ;
    rdfs:label "exhausted" .

HERO:invisibility a HERO:Power ;
    rdfs:label "invisibility" .

HERO:lifestyle a HERO:ThreatTarget ;
    rdfs:label "lifestyle" .

