HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" ;
    rdfs:subClassOf sem:Event .

HERO:Party rdfs:subClassOf sem:Event .

HERO:Raventree_Hall a sem:Place ;
    rdfs:label "Raventree Hall" .

HERO:RefusalOfCall rdfs:subClassOf sem:Event .

HERO:Resurrection rdfs:subClassOf sem:Event .

HERO:Reward rdfs:subClassOf sem:Event .

HERO:RoadBack rdfs:subClassOf sem:Event .

HERO:Seagard a sem:Place ;
    rdfs:label "Seagard" .

