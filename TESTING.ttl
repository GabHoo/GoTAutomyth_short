@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Lannister ;
    HERO:hasOccupation HERO:advisor ;
    HERO:hasTitle HERO:queen_regnant ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Cersei_Lannister .

HERO:Event_02 a HERO:CallToAdventure,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain threatens the safety of somebody. This is the call for the hero" ;
    HERO:Threatens HERO:peace ;
    HERO:journeyStage "2" ;
    HERO:villain HERO:Jon_Snow ;
    sem:hasActor HERO:Cersei_Lannister .

HERO:Event_03 a HERO:MeetingTheMentor,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Meeting the Mentor" ;
    HERO:journeyStage "3" ;
    HERO:meetsMentor HERO:Mace_Tyrell ;
    HERO:powerLearned HERO:mindReading ;
    sem:hasActor HERO:Cersei_Lannister .

HERO:Event_04 a HERO:EncounterAlly,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The hero meets ally" ;
    HERO:journeyStage "4" ;
    HERO:meetsAlly HERO:Joanna_Lannister ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:The_Eyrie ;
    sem:hasTime HERO:the_Autumnal_Equinox .

HERO:Event_05 a HERO:FinalBattle,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Final confrontation between hero and villain" ;
    HERO:helpedBy HERO:Joanna_Lannister ;
    HERO:herofights HERO:Jon_Snow ;
    HERO:journeyStage "5" ;
    HERO:usepower HERO:mindReading ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:Kings_Landing ;
    sem:hasTime HERO:the_Autumnal_Equinox .

HERO:Event_06 a HERO:Party,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero throws a party" ;
    HERO:celebratesvictory HERO:ball ;
    HERO:journeyStage "6" ;
    HERO:partywith HERO:Joanna_Lannister ;
    HERO:saves HERO:castle ;
    sem:hasActor HERO:Cersei_Lannister .

HERO:Threatens rdfs:range HERO:ThreatTarget .

HERO:cause_of_death rdfs:range xsd:string .

HERO:celebratesvictory rdfs:range HERO:Celebration .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:helpedBy rdfs:range HERO:HeroAlly .

HERO:herofights rdfs:range HERO:Villain .

HERO:meetsAlly rdfs:range HERO:HeroAlly .

HERO:meetsMentor rdfs:range HERO:Mentor .

HERO:partywith rdfs:range HERO:HeroAlly .

HERO:powerLearned rdfs:range HERO:HeroPower .

HERO:saves rdfs:range HERO:ThreatTarget .

HERO:usepower rdfs:range HERO:HeroPower .

HERO:villain rdfs:range HERO:Villain .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:CallToAdventure rdfs:subClassOf sem:Event .

HERO:EncounterAlly rdfs:subClassOf sem:Event .

HERO:FinalBattle rdfs:subClassOf sem:Event .

HERO:House_Lannister a HERO:House ;
    rdfs:label "House Lannister" .

HERO:Kings_Landing a sem:Place ;
    rdfs:label "Kings Landing" .

HERO:Mace_Tyrell a HERO:Male,
        sem:Actor ;
    rdfs:label "Mace Tyrell" .

HERO:MeetingTheMentor rdfs:subClassOf sem:Event .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" ;
    rdfs:subClassOf sem:Event .

HERO:Party rdfs:subClassOf sem:Event .

HERO:The_Eyrie a sem:Place ;
    rdfs:label "The Eyrie" .

HERO:advisor a HERO:Occupation ;
    rdfs:label "an advisor" .

HERO:ball a HERO:Celebration ;
    rdfs:label "throwing a ball" .

HERO:castle a HERO:ThreatTarget ;
    rdfs:label "the castle" .

HERO:peace a HERO:ThreatTarget ;
    rdfs:label "the peace" .

HERO:queen_regnant a HERO:Female,
        HERO:Title,
        sem:Actor ;
    rdfs:label "Queen regnant " .

HERO:Jon_Snow a HERO:Main_Character,
        HERO:Male,
        sem:Actor ;
    rdfs:label "Jon Snow" .

HERO:mindReading a HERO:Power ;
    rdfs:label "mind Reading" .

HERO:the_Autumnal_Equinox a sem:Time ;
    rdfs:label "the Autumnal Equinox" .

HERO:Joanna_Lannister a HERO:Female,
        sem:Actor ;
    rdfs:label "Joanna Lannister" .

HERO:Cersei_Lannister a HERO:Female,
        HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Cersei Lannister" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

