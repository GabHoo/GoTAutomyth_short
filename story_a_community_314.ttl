    rdfs:label "hurt" .

HERO:mindReading a HERO:Power ;
    rdfs:label "mind Reading" .

HERO:nightVision a HERO:Power ;
    rdfs:label "night Vision" .

HERO:revolutionary a HERO:Occupation ;
    rdfs:label "revolutionary" .

