HERO:Rickon_Stark a HERO:Male,
        sem:Actor ;
    rdfs:label "Rickon Stark" .

HERO:Riverlands a sem:Place ;
    rdfs:label "Riverlands" .

HERO:RoadBack rdfs:subClassOf sem:Event .

HERO:TestAlly rdfs:subClassOf sem:Event .

HERO:The_Dreadfort a sem:Place ;
    rdfs:label "The Dreadfort" .

HERO:The_Trident a sem:Place ;
    rdfs:label "The Trident" .

HERO:The_Twins a sem:Place ;
    rdfs:label "The Twins" .

HERO:Viserys_Targaryen a HERO:Male,
        sem:Actor ;
    rdfs:label "Viserys Targaryen" .

HERO:christmas a sem:Time ;
    rdfs:label "Christmas" .

HERO:easter a sem:Time ;
    rdfs:label "Easter day" .

HERO:equality a HERO:ThreatTarget ;
    rdfs:label "equality" .

HERO:feast a HERO:Celebration ;
    rdfs:label " having a feast" .

HERO:fireResistance a HERO:Power ;
    rdfs:label "fire Resistance" .

