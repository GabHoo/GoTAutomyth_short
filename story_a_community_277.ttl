HERO:The_Ruby_Ford a sem:Place ;
    rdfs:label "The Ruby Ford" .

HERO:The_Trident a sem:Place ;
    rdfs:label "The Trident" .

HERO:The_Vale_of_Arryn a sem:Place ;
    rdfs:label "The Vale of Arryn" .

HERO:Torrhen_Square a sem:Place ;
    rdfs:label "Torrhen's Square" .

HERO:advisor a HERO:Occupation ;
    rdfs:label "advisor" .

HERO:aggressive a HERO:Feelings ;
    rdfs:label "aggressive" .

