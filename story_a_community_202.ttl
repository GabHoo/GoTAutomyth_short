@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:House a HERO:ThreatTarget,
        rdfs:Class ;
    rdfs:label "House" ;
    rdfs:subClassOf HERO:House .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Tyrell ;
    HERO:hasOccupation HERO:rebel ;
    HERO:hasTitle HERO:King ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Saltpans ;
    sem:hasTime HERO:sunrise .

HERO:Event_02 a HERO:CallToAdventure,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain threatens the safety of somebody. This is the call for the hero" ;
    HERO:Threatens HERO:peace ;
    HERO:journeyStage "2" ;
    HERO:villain HERO:Jon_Snow ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:The_Twins ;
    sem:hasTime HERO:the_Summer_Solstice .

HERO:Event_03 a HERO:RefusalOfCall,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero refuses to help saving the target and the world" ;
    HERO:journeyStage "3" ;
    HERO:refusesToFight HERO:Jon_Snow ;
    HERO:scaredOf HERO:erasingMemory ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Bear_Island ;
    sem:hasTime HERO:the_Summer_Solstice .

HERO:Event_04 a HERO:MeetingTheMentor,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Meeting the Mentor" ;
    HERO:journeyStage "4" ;
    HERO:meetsMentor HERO:Edric_Storm ;
    HERO:powerLearned HERO:erasingMemory ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Oldstones ;
    sem:hasTime HERO:the_Autumnal_Equinox .

HERO:Event_05 a HERO:CrossThreshold,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The hero decides to face the villain and travels to the Place where the villain is." ;
    HERO:journeyStage "5" ;
    HERO:travelsTo HERO:The_Twins ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Riverlands ;
    sem:hasTime HERO:the_Autumnal_Equinox .

HERO:Event_06 a HERO:TestAlly,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The ally help the hero to face the challenge against the Villain and its ally. it is a firt test" ;
    HERO:faces HERO:Rickard_Stark ;
    HERO:journeyStage "6" ;
    HERO:meetsAlly HERO:Catelyn_Stark ;
    HERO:victory false ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Great_Sept_of_Baelor ;
    sem:hasTime HERO:sunrise .

HERO:Event_07 a HERO:InmostCave,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The hero is arrived at the location to face the villain and feel strong emotions" ;
    HERO:feeling HERO:bad ;
    HERO:journeyStage "7" ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Great_Sept_of_Baelor ;
    sem:hasTime HERO:the_Summer_Solstice .

HERO:Event_08 a HERO:Ordeal,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero fights the enemy with powers" ;
    HERO:enemyusespower HERO:slowingTime ;
    HERO:herofights HERO:Jon_Snow ;
    HERO:herogetsinjured false ;
    HERO:herousespower HERO:nightVision ;
    HERO:journeyStage "8" ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Karhold ;
    sem:hasTime HERO:easter .

HERO:Event_09 a HERO:Reward,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero  SAVES WHATEVER WAS THE TARGET OF THE THREAT SO HE GETS IT BACK " ;
    HERO:journeyStage "9" ;
    HERO:saves HERO:House ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Karhold ;
    sem:hasTime HERO:christmas .

HERO:Event_10 a HERO:RoadBack,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain chases hero down and seek for final battle" ;
    HERO:chasedBy HERO:Jon_Snow ;
    HERO:journeyStage "10" ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:Moat_Cailin ;
    sem:hasTime HERO:sunrise .

HERO:Event_11 a HERO:Resurrection,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Final confrontation between hero and villain" ;
    HERO:against HERO:Jon_Snow ;
    HERO:journeyStage "11" ;
    HERO:killsVillain false ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:The_Twins ;
    sem:hasTime HERO:the_first_day_of_Spring .

HERO:Event_12 a HERO:Party,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero throws a party" ;
    HERO:celebratesvictory HERO:show ;
    HERO:journeyStage "12" ;
    HERO:partywith HERO:Catelyn_Stark ;
    sem:hasActor HERO:Bran_Stark ;
    sem:hasPlace HERO:The_Twins ;
    sem:hasTime HERO:christmas .

HERO:Threatens rdfs:range HERO:ThreatTarget .

HERO:against rdfs:range HERO:Villain .

HERO:cause_of_death rdfs:range xsd:string .

HERO:celebratesvictory rdfs:range HERO:Celebration .

HERO:chasedBy rdfs:range HERO:Villain .

HERO:enemyusespower rdfs:range HERO:EnemyPower .

HERO:faces rdfs:range HERO:VillainAlly .

HERO:feeling rdfs:range HERO:Feelings .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:herofights rdfs:range HERO:Villain .

HERO:herogetsinjured rdfs:range xsd:boolean .

HERO:herousespower rdfs:range HERO:HeroPower .

HERO:killsVillain rdfs:range xsd:boolean .

HERO:meetsAlly rdfs:range HERO:HeroAlly .

HERO:meetsMentor rdfs:range HERO:Mentor .

HERO:partywith rdfs:range HERO:HeroAlly .

HERO:powerLearned rdfs:range HERO:HeroPower .

HERO:refusesToFight rdfs:range HERO:Villain .

HERO:saves rdfs:range HERO:ThreatTarget .

HERO:scaredOf rdfs:range HERO:EnemyPower .

HERO:travelsTo rdfs:range sem:Place .

HERO:victory rdfs:range xsd:boolean .

HERO:villain rdfs:range HERO:Villain .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

