HERO:easter a sem:Time ;
    rdfs:label "Easter day" .

HERO:equality a HERO:ThreatTarget ;
    rdfs:label "equality" .

HERO:erasingMemory a HERO:Power ;
    rdfs:label "erasing Memory" .

HERO:exhausted a HERO:Feelings ;
    rdfs:label "exhausted" .

HERO:fictional_princess a HERO:Female,
        HERO:Title,
        sem:Actor ;
    rdfs:label "Princess" .

HERO:invisibility a HERO:Power ;
    rdfs:label "invisibility" .

HERO:theMiddleOftheNight a sem:Time ;
    rdfs:label "the Middle Of the Night" .

HERO:the_Autumnal_Equinox a sem:Time ;
    rdfs:label "the Autumnal Equinox" .

HERO:the_Summer_Solstice a sem:Time ;
    rdfs:label "the Summer Solstice" .

HERO:warrior a HERO:Occupation ;
    rdfs:label "warrior" .

HERO:Joffrey_Baratheon a HERO:Male,
        sem:Actor ;
    rdfs:label "Joffrey Baratheon" .

HERO:Moat_Cailin a sem:Place ;
    rdfs:label "Moat Cailin" .

HERO:Saltpans a sem:Place ;
    rdfs:label "Saltpans" .

HERO:teleportation a HERO:Power ;
    rdfs:label "teleportation" .

HERO:Red_Keep a sem:Place ;
    rdfs:label "Red Keep" .

HERO:sunrise a sem:Time ;
    rdfs:label "sunrise" .

