@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Stark ;
    HERO:hasOccupation HERO:rebel ;
    HERO:hasTitle HERO:fictional_king ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Margaery_Tyrell ;
    sem:hasPlace HERO:The_Trident ;
    sem:hasTime HERO:the_Autumnal_Equinox .

HERO:Event_02 a HERO:CallToAdventure,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Villain threatens the safety of somebody. This is the call for the hero" ;
    HERO:ThreatenedElement HERO:castle ;
    HERO:journeyStage "2" ;
    HERO:villain HERO:Tommen_Baratheon ;
    sem:hasActor HERO:Margaery_Tyrell ;
    sem:hasPlace HERO:The_Ruby_Ford ;
    sem:hasTime HERO:easter .

HERO:ThreatenedElement rdfs:range HERO:ThreatTarget .

HERO:cause_of_death rdfs:range xsd:string .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:villain rdfs:range HERO:Villain .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:CallToAdventure rdfs:label "Second stage of the story - Call to adventure" .

HERO:House_Stark a HERO:House ;
    rdfs:label "House Stark" .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" .

HERO:The_Ruby_Ford a sem:Place ;
    rdfs:label "The Ruby Ford" .

HERO:The_Trident a sem:Place ;
    rdfs:label "The Trident" .

HERO:Tommen_Baratheon a HERO:Male,
        sem:Actor ;
    rdfs:label "Tommen Baratheon" .

HERO:castle a HERO:ThreatTarget ;
    rdfs:label "castle" .

HERO:easter a sem:Time ;
    rdfs:label "Easter day" .

HERO:fictional_king a HERO:Male,
        HERO:Title,
        sem:Actor ;
    rdfs:label "King" .

HERO:rebel a HERO:Occupation ;
    rdfs:label "rebel" .

HERO:the_Autumnal_Equinox a sem:Time ;
    rdfs:label "the Autumnal Equinox" .

HERO:Margaery_Tyrell a HERO:Female,
        HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Margaery Tyrell" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

