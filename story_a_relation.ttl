@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Tully ;
    HERO:hasOccupation HERO:vigilante ;
    HERO:hasTitle HERO:queen_consort ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Arya_Stark ;
    sem:hasPlace HERO:Kings_Landing ;
    sem:hasTime HERO:sunrise .

HERO:cause_of_death rdfs:range xsd:string .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:Arya_Stark a HERO:Female,
        HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Arya Stark" .

HERO:House_Tully a HERO:House ;
    rdfs:label "House Tully" .

HERO:Kings_Landing a sem:Place ;
    rdfs:label "Kings Landing" .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" .

HERO:queen_consort a HERO:Female,
        HERO:Title,
        sem:Actor ;
    rdfs:label "Queen " .

HERO:sunrise a sem:Time ;
    rdfs:label "sunrise" .

HERO:vigilante a HERO:Occupation ;
    rdfs:label "vigilante" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

