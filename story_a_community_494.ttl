HERO:Robb_Stark a HERO:Male,
        sem:Actor ;
    rdfs:label "Robb Stark" .

HERO:Saltpans a sem:Place ;
    rdfs:label "Saltpans" .

HERO:Seagard a sem:Place ;
    rdfs:label "Seagard" .

HERO:Stannis_Baratheon a HERO:Male,
        sem:Actor ;
    rdfs:label "Stannis Baratheon" .

HERO:TestAlly rdfs:subClassOf sem:Event .

HERO:The_Dreadfort a sem:Place ;
    rdfs:label "The Dreadfort" .

HERO:The_Eyrie a sem:Place ;
    rdfs:label "The Eyrie" .

HERO:The_Iron_Island a sem:Place ;
    rdfs:label "The Iron Island" .

HERO:Winterfell a sem:Place ;
    rdfs:label "Winterfell" .

HERO:easter a sem:Time ;
    rdfs:label "Easter day" .

HERO:fictional_king a HERO:Male,
        HERO:Title,
        sem:Actor ;
    rdfs:label "King" .

HERO:game a HERO:Celebration ;
    rdfs:label "playing a game" .

HERO:pausingTime a HERO:Power ;
    rdfs:label "pausing Time" .

HERO:procurer a HERO:Occupation ;
    rdfs:label "procurer" .

HERO:teleportation a HERO:Power ;
    rdfs:label "teleportation" .

HERO:upset a HERO:Feelings ;
    rdfs:label "upset" .

HERO:Khal_Drogo a HERO:Male,
        sem:Actor ;
    rdfs:label "Khal Drogo" .

HERO:Red_Keep a sem:Place ;
    rdfs:label "Red Keep" .

HERO:The_Twins a sem:Place ;
    rdfs:label "The Twins" .

HERO:nightVision a HERO:Power ;
    rdfs:label "night Vision" .

