    rdfs:label "The Iron Island" .

HERO:The_Ruby_Ford a sem:Place ;
    rdfs:label "The Ruby Ford" .

HERO:The_Trident a sem:Place ;
    rdfs:label "The Trident" .

HERO:The_Whispering_Wood a sem:Place ;
    rdfs:label "The Whispering Wood" .

HERO:disgusted a HERO:Feelings ;
    rdfs:label "disgusted" .

HERO:feast a HERO:Celebration ;
