@prefix HERO: <http://hero_ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

HERO:Event_01 a HERO:OrdinaryWorld,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Ordinary World presentation" ;
    HERO:hasHouse HERO:House_Stark ;
    HERO:hasOccupation HERO:hunter ;
    HERO:hasTitle HERO:fictional_princess ;
    HERO:journeyStage "1" ;
    sem:hasActor HERO:Robert_Baratheon ;
    sem:hasPlace HERO:Forest ;
    sem:hasTime HERO:easter .

HERO:Event_04 a HERO:MeetingTheMentor,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Meeting the Mentor" ;
    HERO:journeyStage "4" ;
    HERO:meetsMentor HERO:Mace_Tyrell ;
    HERO:powerLearned HERO:mindReading ;
    sem:hasActor HERO:Robert_Baratheon ;
    sem:hasPlace HERO:Winterfell ;
    sem:hasTime HERO:afternoon .

HERO:cause_of_death rdfs:range xsd:string .

HERO:hasHouse rdfs:range HERO:House .

HERO:hasOccupation rdfs:range HERO:Occupation .

HERO:hasTitle rdfs:range HERO:Title .

HERO:meetsMentor rdfs:range HERO:Mentor .

HERO:powerLearned rdfs:range HERO:HeroPower .

sem:hasActor rdfs:range HERO:Hero .

sem:hasPlace rdfs:range sem:Place .

sem:hasTime rdfs:range sem:Time .

HERO:Forest a sem:Place ;
    rdfs:label "Forest" .

HERO:House_Stark a HERO:House ;
    rdfs:label "House Stark" .

HERO:Mace_Tyrell a sem:Actor ;
    rdfs:label "Mace Tyrell" .

HERO:OrdinaryWorld rdfs:label "First stage of the story - Ordinary world presentation" .

HERO:Winterfell a sem:Place ;
    rdfs:label "Winterfell" .

HERO:afternoon a sem:Time ;
    rdfs:label "afternoon" .

HERO:easter a sem:Time ;
    rdfs:label "Easter" .

HERO:fictional_princess a HERO:Title ;
    rdfs:label "Princess" .

HERO:hunter a HERO:Occupation ;
    rdfs:label "hunter" .

HERO:mindReading a HERO:Power ;
    rdfs:label "mind Reading" .

HERO:Robert_Baratheon a HERO:Main_Character,
        sem:Actor ;
    rdfs:label "Robert Baratheon" .

sem:Core a owl:Class ;
    rdfs:label "Core" .

sem:Event a owl:Class ;
    rdfs:label "Event" .

