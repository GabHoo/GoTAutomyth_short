HERO:fireResistance a HERO:Power ;
    rdfs:label "fire Resistance" .

HERO:freedom a HERO:ThreatTarget ;
    rdfs:label "freedom" .

HERO:invisibility a HERO:Power ;
    rdfs:label "invisibility" .

HERO:mindReading a HERO:Power ;
    rdfs:label "mind Reading" .

