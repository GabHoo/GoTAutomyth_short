    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:Castle_Black ;
    sem:hasTime HERO:theMiddleOftheNight .

HERO:Event_05 a HERO:CrossThreshold,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The hero decides to face the villain and travels to the Place where the villain is." ;
    HERO:journeyStage "5" ;
    HERO:travelsTo HERO:Bear_Island ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:Moat_Cailin ;
    sem:hasTime HERO:the_first_day_of_Spring .

HERO:Event_06 a HERO:TestAlly,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The ally help the hero to face the challenge against the Villain and its ally. it is a firt test" ;
    HERO:faces HERO:Joffrey_Baratheon ;
    HERO:journeyStage "6" ;
    HERO:meetsAlly HERO:Joanna_Lannister ;
    HERO:victory true ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:Kings_Landing ;
    sem:hasTime HERO:the_Summer_Solstice .

HERO:Event_07 a HERO:InmostCave,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "The hero is arrived at the location to face the villain and feel strong emotions" ;
    HERO:feeling HERO:frustrated ;
    HERO:journeyStage "7" ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:The_Iron_Island ;
    sem:hasTime HERO:the_first_day_of_Spring .

HERO:Event_08 a HERO:Ordeal,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
    rdfs:label "Hero fights the enemy with powers" ;
    HERO:enemyusespower HERO:mindReading ;
    HERO:herofights HERO:Margaery_Tyrell ;
    HERO:herogetsinjured true ;
    HERO:herousespower HERO:erasingMemory ;
    HERO:journeyStage "8" ;
    sem:hasActor HERO:Cersei_Lannister ;
    sem:hasPlace HERO:The_Twins ;
    sem:hasTime HERO:christmas .

HERO:Event_09 a HERO:Reward,
        sem:Core,
        sem:Event,
        rdfs:Resource ;
