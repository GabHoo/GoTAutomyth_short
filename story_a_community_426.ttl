HERO:TestAlly rdfs:subClassOf sem:Event .

HERO:The_Twins a sem:Place ;
    rdfs:label "The Twins" .

HERO:The_Whispering_Wood a sem:Place ;
    rdfs:label "The Whispering Wood" .

HERO:erasingMemory a HERO:Power ;
    rdfs:label "erasing Memory" .

HERO:invisibility a HERO:Power ;
    rdfs:label "invisibility" .

HERO:jewel a HERO:ThreatTarget ;
    rdfs:label "jewel" .

HERO:lovesick a HERO:Feelings ;
    rdfs:label "lovesick" .

HERO:mindReading a HERO:Power ;
    rdfs:label "mind Reading" .

HERO:pausingTime a HERO:Power ;
    rdfs:label "pausing Time" .

